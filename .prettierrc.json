{
  "plugins": ["prettier-plugin-tailwindcss"],
  "singleQuote": true,
  "jsxSingleQuote": false,
  "tabWidth": 2,
  "trailingComma": "all",
  "useTabs": false,
  "endOfLine": "lf",
  "bracketSameLine": false,
  "printWidth": 80,
  "semi": true,
  "arrowParens": "always",
  "bracketSpacing": true,
  "proseWrap": "preserve"
}
